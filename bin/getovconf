#!/bin/ksh

# Get the Openview configuration
OS=`uname`

# need to be root:
RUNBY=`/usr/bin/id -u`
if [[ ! $RUNBY -eq 0 ]]
then
  echo "This script must be run by root."
  exit 0
fi

if [[ -f /opt/intrado/etc/getovconf.output ]]
then
  rm /opt/intrado/etc/getovconf.output
fi

if [[ -f /opt/OV/bin/ovconfget ]]
then
  /opt/OV/bin/ovconfget > /opt/intrado/etc/getovconf.output
fi

